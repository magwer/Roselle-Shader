	#define NIGHTSKY_COLOR vec3(NIGHT_SKY_COLOR_R, NIGHT_SKY_COLOR_G, NIGHT_SKY_COLOR_B)
	
	const vec3 morningSkyColor = pow(MORNINGSKY_COLOR, vec3(2.2f));
	const vec3 daySkyColor = pow(DAYSKY_COLOR, vec3(2.2f));
	const vec3 nightSkyColor = pow(NIGHTSKY_COLOR, vec3(2.2f));
#endif

const vec3 morningLight = MORNINGLIGHT_COLOR * MORNINGLIGHT_STRENGTH;
const vec3 morningAmbient = MORNINGAMBIENT_COLOR * MORNINGAMBIENT_STRENGTH;
const vec3 dayLight = DAYLIGHT_COLOR * DAYLIGHT_STRENGTH;
const vec3 dayAmbient = DAYAMBIENT_COLOR * DAYAMBIENT_STRENGTH;
const vec3 nightLight = NIGHTLIGHT_COLOR * NIGHTLIGHT_STRENGTH;
const vec3 nightAmbient = NIGHTAMBIENT_COLOR * NIGHTAMBIENT_STRENGTH;
#ifdef FOG
	uniform float blindness;
#endif

float nightMul;
float morningMul;

vec3 calLightColor(vec3 morning, vec3 day, vec3 night) {
	return mix(night, mix(day, morning, morningMul), 1 - nightMul);
}

float calStrength(float morning, float day, float night) {
	return mix(night, mix(day, morning, morningMul), 1 - nightMul);
}

void main() {

	gl_Position = ftransform();
	texcoord.st = gl_MultiTexCoord0.st;

	nightMul = clamp((worldTime - 12000) / 1000.0f, 0, 1) - clamp((worldTime - 23000) / 1000.0f, 0, 1);
  	texcoord.b = 1 - (clamp((worldTime - 12000) / 500.0f, 0, 1) - clamp((worldTime - 13000) / 500.0f, 0, 1) + clamp((worldTime - 22500) / 500.0f, 0, 1) - clamp((worldTime - 23250) / 500.0f, 0, 1));
	morningMul = 1 - clamp((12500 - worldTime) / 1000.0f, 0, 1) - clamp((worldTime - 23500) / 1000.0f, 0, 1) - clamp((worldTime - 500) / 1000.0f, -0.5f, 0);

	mag_directLight = calLightColor(morningLight, dayLight, nightLight);
	mag_ambientLight = calLightColor(morningAmbient, dayAmbient, nightAmbient);
	#ifdef WATER_SKY_REFLECTION
		mag_skyColorOrigin = calLightColor(morningSkyColor, daySkyColor, nightSkyColor);
	#endif
	#ifdef SUNBLUR
		mag_sunBlurColor = calLightColor(morningSunBlur, daySunBlur, nightSunBlur);
	#endif
	#ifdef DESATURATION
		mag_nightMul = nightMul;
	#endif
	#ifdef FOG
		#ifdef WEATHER_FOG
			mag_fogColorOrigin = calLightColor(MORNINGFOG_COLOR, DAYFOG_COLOR, NIGHTFOG_COLOR);
		  	mag_fogStrengthOrigin = calStrength(MORNINGFOG_STRENGTH, DAYFOG_STRENGTH, NIGHTFOG_STRENGTH);
			mag_fogFarMulOrigin = calStrength(MORNINGFOG_FAR, DAYFOG_FAR, NIGHTFOG_FAR);
		#else
			mag_fogColorOrigin = mix(calLightColor(MORNINGFOG_COLOR, DAYFOG_COLOR, NIGHTFOG_COLOR), BLINDNESSFOG_COLOR, blindness);
		  	mag_fogStrengthOrigin = mix(calStrength(MORNINGFOG_STRENGTH, DAYFOG_STRENGTH, NIGHTFOG_STRENGTH), BLINDNESSFOG_STRENGTH, blindness);
			mag_fogFarMulOrigin = mix(calStrength(MORNINGFOG_FAR, DAYFOG_FAR, NIGHTFOG_FAR), BLINDNESSFOG_FAR, blindness);
		#endif
	#endif
}
